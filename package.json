{
  "name": "modules-cdn-webpack-plugin",
  "version": "3.2.0",
  "description": "Dynamically get your dependencies from a cdn rather than bundling them in your app",
  "license": "MIT",
  "repository": "mastilver/modules-cdn-webpack-plugin",
  "author": {
    "name": "Thomas Sileghem",
    "email": "th.sileghem@gmail.com",
    "url": "mastilver.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && nyc ava",
    "prepublish": "babel src --out-dir lib",
    "add-contributor": "all-contributors add"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "keywords": [
    "webpack",
    "cdn",
    "unpkg.com",
    "html-webpack-plugin",
    "webpack-manifest-plugin",
    "html",
    "script",
    "assets",
    "externals"
  ],
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "html-webpack-include-assets-plugin": "0.0.5",
    "read-pkg-up": "^2.0.0",
    "resolve-pkg": "^1.0.0"
  },
  "peerDependencies": {
    "module-to-cdn": "^3.0.1",
    "webpack": "2.x"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.3.0",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "codecov": "^2.2.0",
    "html-webpack-plugin": "^2.28.0",
    "module-to-cdn": "^3.0.4",
    "mz": "^2.6.0",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-manifest-plugin": "^1.1.0",
    "xo": "^0.18.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "rules": {
      "eqeqeq": "off",
      "no-eq-null": "off"
    }
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fixtures/output/**/*"
    ],
    "babel": "inherit",
    "require": [
      "babel-core/register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
